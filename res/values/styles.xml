<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="TwTabWidget">
        <item name="android:textAppearance">@style/TwTextAppearanceTab</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="TwAlertDialog">
        <item name="fullDark">@drawable/tw_popup_full_dark</item>
        <item name="topDark">@drawable/tw_popup_top_dark</item>
        <item name="centerDark">@drawable/tw_popup_center_dark</item>
        <item name="bottomDark">@drawable/tw_popup_bottom_dark</item>
        <item name="fullBright">@drawable/tw_popup_full_bright</item>
        <item name="topBright">@drawable/tw_popup_top_bright</item>
        <item name="centerBright">@drawable/tw_popup_center_bright</item>
        <item name="bottomBright">@drawable/tw_popup_bottom_bright</item>
        <item name="bottomMedium">@drawable/tw_popup_bottom_medium</item>
        <item name="centerMedium">@drawable/tw_popup_center_medium</item>
        <item name="topBrightWhite">@drawable/tw_popup_top_bright_white</item>
    </style>
    <style name="TwProgressBar">
        <item name="android:maxHeight">14.0dip</item>
        <item name="android:indeterminateOnly">false</item>
        <item name="android:progressDrawable">@drawable/tw_progress_horizontal</item>
        <item name="android:minHeight">14.0dip</item>
    </style>
    <style name="TwDialogWindowTitle">
        <item name="android:textAppearance">@android:style/TextAppearance.DialogWindowTitle</item>
        <item name="android:maxLines">1</item>
        <item name="android:scrollHorizontally">true</item>
    </style>
    <style name="TwAbstractRadioCheckButton">
        <item name="android:textAppearance">@style/TwTextAppearanceListOneLine</item>
        <item name="android:gravity">left|center</item>
        <item name="android:background">@android:drawable/list_selector_background</item>
        <item name="android:paddingLeft">10.0dip</item>
        <item name="android:paddingTop">10.0dip</item>
        <item name="android:paddingRight">10.0dip</item>
        <item name="android:paddingBottom">10.0dip</item>
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
        <item name="buttonDrawable">@android:drawable/btn_radio</item>
        <item name="buttonDrawablePaddingLeft">10.0dip</item>
        <item name="buttonDrawablePaddingTop">5.0dip</item>
        <item name="buttonDrawablePaddingRight">10.0dip</item>
        <item name="buttonDrawablePaddingBottom">5.0dip</item>
        <item name="buttonDrawableGravity">right|center</item>
    </style>
    <style name="TwAbstractRadioCheckButton.TwRadioButton" parent="@style/TwAbstractRadioCheckButton" />
    <style name="TwLeftRadioButtonStyle" parent="@style/TwAbstractRadioCheckButton.TwRadioButton">
        <item name="android:paddingLeft">60.0dip</item>
        <item name="buttonDrawable">@drawable/tw_btn_radio</item>
        <item name="buttonDrawableGravity">left|center</item>
    </style>
    <style name="TwRightRadioButtonStyle" parent="@style/TwAbstractRadioCheckButton.TwRadioButton">
        <item name="android:paddingRight">60.0dip</item>
        <item name="buttonDrawable">@drawable/tw_btn_radio</item>
        <item name="buttonDrawableGravity">right|center</item>
    </style>
    <style name="TwSoftkeyItemStyle">
        <item name="android:textAppearance">@style/TwTextAppearanceSoftkey</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingTop">0.0dip</item>
        <item name="android:paddingBottom">0.0dip</item>
        <item name="android:scaleType">center</item>
        <item name="android:maxLines">2</item>
    </style>
    <style name="TwTextAppearanceTitle">
        <item name="android:textSize">13.329987sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/tw_textappearance_title</item>
    </style>
    <style name="TwTextAppearanceTab">
        <item name="android:textSize">13.329987sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@color/tw_textappearance_tab</item>
    </style>
    <style name="TwTextAppearanceMenuSmall">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@color/tw_textappearance_menusmall</item>
    </style>
    <style name="TwTextAppearanceSoftkey">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@color/tw_textappearance_softkey</item>
    </style>
    <style name="TwTextAppearanceListOneLine">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@color/tw_textappearance_listoneline</item>
    </style>
    <style name="TwTextAppearanceListTwoLinePrimary">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@color/tw_textappearance_listtwolineprimary</item>
    </style>
    <style name="TwTextAppearanceListTwoLineSecondary">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@color/tw_textappearance_listtwolinesecondary</item>
    </style>
    <style name="TwTextAppearanceListTwoLineSecondaryOrange">
        <item name="android:textSize">13.329987sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">#ff006aaa</item>
    </style>
    <style name="TwTextAppearanceListTwoLineSecondaryGrey">
        <item name="android:textSize">13.329987sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@color/tw_color047</item>
    </style>
    <style name="TwTextAppearanceGroupTitle">
        <item name="android:textSize">13.329987sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/tw_textappearance_grouptitle</item>
    </style>
    <style name="TwAbsSeekBar" parent="@style/TwProgressBar" />
    <style name="TwSeekBar" parent="@style/TwAbsSeekBar">
        <item name="android:focusable">true</item>
        <item name="android:maxHeight">11.0dip</item>
        <item name="android:indeterminateOnly">false</item>
        <item name="android:progressDrawable">@drawable/tw_progress_horizontal</item>
        <item name="android:minHeight">11.0dip</item>
        <item name="android:thumb">@drawable/tw_seek_thumb</item>
        <item name="android:thumbOffset">0.0px</item>
        <item name="twIndicatorThickness">14.0dip</item>
        <item name="twSeekThumbFontColor">@color/tw_color140</item>
        <item name="twSeekThumbFontSize">15.0sp</item>
        <item name="twSeekThumbFontEnable">false</item>
    </style>
    <style name="TwTheme" parent="@android:style/Theme.DeviceDefault">
        <item name="twTabWidgetStyle">@style/TwTabWidget</item>
        <item name="twRadioButtonStyle">@style/TwRightRadioButtonStyle</item>
        <item name="twSoftkeyItemStyle">@style/TwSoftkeyItemStyle</item>
        <item name="twProgressBarStyle">@style/TwProgressBar</item>
        <item name="twSeekBarStyle">@style/TwSeekBar</item>
        <item name="TwCircleButtonPlusStyle">@style/TwCircleButtonPlus</item>
        <item name="TwCircleButtonMinusStyle">@style/TwCircleButtonMinus</item>
        <item name="TwCircleButtonPopUpPlusStyle">@style/TwCircleButtonPopUpPlus</item>
        <item name="TwCircleButtonPopUpMinusStyle">@style/TwCircleButtonMinus</item>
        <item name="twWhiteStyleList">@style/TwWhiteStyleList</item>
        <item name="TwIndepthButtonStyle">@style/TwIndepthButton</item>
        <item name="twSearchButtonStyle">@style/TwSearchButton</item>
        <item name="twVoiceSearchButtonStyle">@style/TwVoiceSearchButton</item>
        <item name="twSearchFieldStyle">@style/TwSearchField</item>
        <item name="listChoiceIndicatorMultiple">@drawable/btn_check</item>
        <item name="listChoiceIndicatorSingle">@drawable/btn_radio</item>
    </style>
    <style name="TwTheme.Dialog" parent="@style/TwTheme" />
    <style name="TwTheme.Dialog.Alert" parent="@style/TwTheme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowTitleStyle">@style/TwDialogWindowTitle</item>
    </style>
    <style name="TwTextAppearanceEditTitle">
        <item name="android:textSize">17.329987sp</item>
        <item name="android:textColor">#ffffffff</item>
    </style>
    <style name="TwTextAppearanceEditText">
        <item name="android:textSize">18.659973sp</item>
        <item name="android:textColor">#ff000000</item>
    </style>
    <style name="TwCircleButtonPlus" parent="@android:style/Widget.ImageButton">
        <item name="android:background">@drawable/tw_btn_circle</item>
        <item name="android:src">@drawable/tw_btn_circle_plus_ss</item>
    </style>
    <style name="TwCircleButtonMinus" parent="@android:style/Widget.ImageButton">
        <item name="android:background">@drawable/tw_btn_circle</item>
        <item name="android:src">@drawable/tw_btn_circle_minus_ss</item>
    </style>
    <style name="TwCircleButtonPopUpPlus" parent="@android:style/Widget.ImageButton">
        <item name="android:background">@drawable/tw_btn_circle_popup</item>
        <item name="android:src">@drawable/tw_btn_circle_plus_ss</item>
    </style>
    <style name="TwCircleButtonPopUpMinus" parent="@android:style/Widget.ImageButton">
        <item name="android:background">@drawable/tw_btn_circle_popup</item>
        <item name="android:src">@drawable/tw_btn_circle_minus_ss</item>
    </style>
    <style name="TwIndepthButton" parent="@android:style/Widget.ImageButton">
        <item name="android:background">@drawable/tw_btn_circle</item>
        <item name="android:src">@drawable/tw_btn_indepth</item>
    </style>
    <style name="TwSearchButton" parent="@android:style/Widget.ImageButton">
        <item name="android:background">@drawable/tw_btn_search</item>
        <item name="android:src">@drawable/APKTOOL_DUMMY_0208</item>
    </style>
    <style name="WhiteStyleList" parent="@android:style/Theme.Light">
        <item name="android:listViewStyle">@android:style/Widget.ListView.White</item>
    </style>
    <style name="TwWhiteStyleList" parent="@android:style/Theme.Light">
        <item name="android:listViewStyle">@android:style/Widget.ListView.White</item>
    </style>
    <style name="TwVoiceSearchButton" parent="@android:style/Widget.ImageButton">
        <item name="android:background">@drawable/tw_btn_voice_search</item>
        <item name="android:src">@drawable/APKTOOL_DUMMY_0209</item>
    </style>
    <style name="TwSearchField" parent="@android:style/Widget.AutoCompleteTextView">
        <item name="android:background">@drawable/tw_search_field</item>
    </style>
    <style name="TwBodyButton" parent="@android:style/Widget.Button">
        <item name="android:textStyle">normal</item>
    </style>
    <style name="TwBodyButton.TwTextAppearanceBodyButton01" parent="@style/TwBodyButton">
        <item name="android:textColor">@color/tw_textappearance_body_button_01</item>
    </style>
    <style name="TwBodyButton.TwTextAppearanceBodyButton02" parent="@style/TwBodyButton">
        <item name="android:textColor">@color/tw_textappearance_body_button_02</item>
    </style>
    <style name="TwBodyButton.TwTextAppearanceBodyButton03" parent="@style/TwBodyButton">
        <item name="android:textColor">@color/tw_textappearance_body_button_03</item>
    </style>
    <style name="TwTheme.Dialog.Clipboard" parent="@android:style/Theme.DeviceDefault.Panel">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.InputMethod</item>
        <item name="android:background">@null</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>
</resources>
